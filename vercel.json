
{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*\\.(js|css|svg|png|jpg|jpeg|gif|ico|json))", "dest": "/dist/public/$1" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
